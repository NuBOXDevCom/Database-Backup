{
  "name": "nuboxdevcom/database-backup",
  "description": "Backup script for MySQL databases",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "mysql",
    "library",
    "nuboxdevcom",
    "nubox",
    "backup",
    "database",
    "opensource",
    "free",
    "script"
  ],
  "homepage": "https://www.github.com/NuBOXDevCom/Database-Backup",
  "authors": [
    {
      "name": "NuBOX DevCom",
      "email": "slye@nubox.fr",
      "homepage": "http://www.sylvain-bordier.fr",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "http://github.com/NuBOXDevCom/Database-Backup/issues",
    "source": "https://www.github.com/NuBOXDevCom/Database-Backup"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">= 7.2",
    "ifsnop/mysqldump-php": "^2.6",
    "swiftmailer/swiftmailer": "^6.1",
    "vlucas/phpdotenv": "^3.2",
    "jbzoo/lang": "^1.0",
    "symfony/yaml": "^4.2",
    "ext-pdo": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7.5",
    "fzaninotto/faker": "1.8.*",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "NDC\\DatabaseBackup\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": "php -r \"copy('.env-dist', '.env');\"",
    "coverage": [
      "phpunit --process-isolation --testdox --coverage-html reports/clover_html"
    ],
    "tests": [
      "phpunit --process-isolation --testdox"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 0
  }
}
